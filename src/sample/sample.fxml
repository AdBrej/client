<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.Controller">
<center>
    <TableView fx:id="pathsTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
            <TableColumn fx:id="pathColumn" prefWidth="435.0" text="URL"/>
            <TableColumn fx:id="progressColumn" minWidth="0.0" prefWidth="253.0" text="Progress"/>
            <TableColumn fx:id="statusColumn" minWidth="0.0" prefWidth="110.0" text="Status"/>
        </columns>
    </TableView>
</center>
<bottom>
    <BorderPane prefHeight="43.0" prefWidth="600.0" BorderPane.alignment="CENTER">
        <left>
            <Pane prefHeight="43.0" prefWidth="362.0" BorderPane.alignment="CENTER">
                <children>
                    <Button layoutX="14.0" layoutY="9.0" mnemonicParsing="false" onAction="#addFile" text="Add File"/>
                </children>
            </Pane>
        </left>
        <right>
            <Pane prefHeight="43.0" prefWidth="115.0" BorderPane.alignment="CENTER">
                <children>

                    <Button layoutX="10.0" layoutY="9.0" mnemonicParsing="false" onAction="#send"
                            minWidth="80.0" text="Send File"/>
                </children>
            </Pane>
        </right>
    </BorderPane>
</bottom>
</BorderPane>





